{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "buntywp/author-card",
	"version": "0.1.0",
	"title": "Author Card",
	"category": "widgets",
	"icon": "admin-users",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false,
		"color": {
			"background": true,
			"text": false
		},
		"dimensions": {
			"aspectRatio": true,
			"minHeight": false
		}
	},
	"attributes": {
		"imageUrl": {
			"type": "string"
		},
		"bioText": {
			"type": "string"
		},
		"imgHeight": {
			"type": "string",
			"default": "300"
		},
		"imgWidth":{
			"type": "string",
			"default": "300"
		}
	},
	"textdomain": "author-card",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
